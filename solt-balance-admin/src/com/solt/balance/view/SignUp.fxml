<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="290.0" prefWidth="516.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.solt.balance.view.SignUp">
   <children>
      <HBox alignment="CENTER" prefHeight="68.0" prefWidth="516.0" style="-fx-background-color: gray;">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ADMIN SIGN UP">
               <font>
                  <Font name="AdobeMingStd-Light" size="26.0" />
               </font>
            </Text>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="153.0" prefWidth="516.0">
         <children>
            <GridPane prefHeight="161.0" prefWidth="428.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="229.0" minWidth="10.0" prefWidth="154.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="313.0" minWidth="10.0" prefWidth="313.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Name" />
                  <Label text="Password" GridPane.rowIndex="1" />
                  <Label text="Confirm Password" GridPane.rowIndex="2" />
                  <TextField fx:id="name" GridPane.columnIndex="1" />
                  <PasswordField fx:id="pass1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <PasswordField fx:id="pass2" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="69.0" prefWidth="516.0" spacing="10.0" style="-fx-background-color: skyblue;">
         <children>
            <Button mnemonicParsing="false" onAction="#close" prefHeight="26.0" prefWidth="80.0" text="CLOSE" />
            <Button mnemonicParsing="false" onAction="#signup" prefWidth="80.0" text="SIGN UP" />
         </children>
      </HBox>
   </children>
</VBox>
